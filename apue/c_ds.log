typedef   已有类型  新名字；
typedef  int  INT;		-> int  -> INT
INT i; ->  int i;  

typedef int * INT;      ->  int * -> INT
INT i; ->  int * i;

typedef int  A[6];   ->  int[6]  A;
A a; ->  int a[6];

typedef int A[2][3];

typedef int FUN(int,int);    -> int(int,int) FUN
FUN  f;  ->  int f(int,int)

typedef int *FUN(int*,int*); 
FUN  f;  -> int *f(int*,int*)

typedef int (*FUN)(int*,int*);
FUN f;  -> int (*f)(int*,int*);


typedef int *(*FUN)(int*,int*);

typedef int *(*FUN[3])(int*,int*);
FUN f;  -> int *(*f[3])(int*,int*);
		->  int*(*)(int*,int*)   [3]   f;

